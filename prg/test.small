
jmp main


lab swap
    pla
    sta retaddr

    pla
    sta x
    pla
    sta y
    lda x
    pha
    lda y
    pha

    lda retaddr
    pha
    ret


lab not
    cal swap
    pla

    sta x
    nad x

    pha
    cal swap
    ret


lab and
    cal swap
    pla
    sta b
    cal swap
    pla
    sta a

    lda a
    nad b
    pha
    cal not

    cal swap
    ret





lab main

    ldi 64
    pha
    ldi 1
    pha
    cal and
    pla
    out

    hlt
