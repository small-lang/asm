
jmp main
use 'lib/stack.small'


lab not
    cal stack::swap
    pla

    sta x
    nad x

    pha
    cal stack::swap
    ret


lab and
    cal stack::swap
    pla
    sta b
    cal stack::swap
    pla
    sta a

    lda a
    nad b
    pha
    cal not

    cal stack::swap
    ret





lab main

    ldi 64
    pha
    ldi 1
    pha
    cal and
    pla
    out

    hlt
